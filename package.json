{
  "name": "bearmentor",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "typecheck": "tsc",
    "format": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "format:fix": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "lint": "eslint \"app/**/*.+(ts|tsx)\"",
    "lint:fix": "eslint \"app/**/*.+(ts|tsx)\" --fix",
    "stylelint": "stylelint ./app/*.css",
    "stylelint:fix": "stylelint --fix ./app/*.css",
    "check": "pnpm format && pnpm stylelint && pnpm lint && pnpm typecheck && pnpm build",
    "check:fix": "pnpm format:fix && pnpm stylelint && pnpm lint:fix",
    "db:gen": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:check": "tsx prisma/check.ts",
    "db:studio": "prisma studio --browser none",
    "reveal": "remix reveal",
    "clean": "rm -rf build public/build",
    "prebuild": "prisma db push",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@ariakit/react": "0.2.12",
    "@conform-to/react": "0.7.4",
    "@conform-to/zod": "0.7.4",
    "@fontsource/anybody": "5.0.5",
    "@fontsource/pt-mono": "5.0.5",
    "@fontsource/pt-sans": "5.0.5",
    "@hookform/resolvers": "3.1.1",
    "@prisma/client": "5.0.0",
    "@radix-ui/react-avatar": "1.0.3",
    "@radix-ui/react-collapsible": "1.0.3",
    "@radix-ui/react-hover-card": "1.0.6",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-scroll-area": "1.0.4",
    "@radix-ui/react-select": "1.2.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-toast": "1.1.4",
    "@radix-ui/react-tooltip": "1.0.6",
    "@remix-run/css-bundle": "^1.19.1",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/vercel": "^1.19.1",
    "@vercel/analytics": "1.0.1",
    "@vercel/node": "^2.15.6",
    "array-shuffle": "3.0.0",
    "bcryptjs": "2.4.3",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "isbot": "^3.6.13",
    "nanoid": "3.3.4",
    "nprogress": "0.2.0",
    "pluralize": "8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "3.5.0",
    "remix-auth-form": "1.3.0",
    "remix-utils": "6.6.0",
    "tailwind-merge": "1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "1.0.6",
    "tiny-invariant": "1.3.1",
    "usehooks-ts": "2.9.1",
    "voca": "1.4.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@remix-run/serve": "^1.19.1",
    "@tailwindcss/typography": "0.5.9",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "20.4.4",
    "@types/nprogress": "0.2.0",
    "@types/pluralize": "0.0.30",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/voca": "1.4.2",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@typescript-eslint/typescript-estree": "6.1.0",
    "autoprefixer": "10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "8.8.0",
    "postcss": "8.4.27",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.4.1",
    "prisma": "5.0.0",
    "stylelint": "15.10.2",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "taze": "0.11.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "3.12.7",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
